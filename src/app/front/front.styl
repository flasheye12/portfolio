.front-container
  margin-top: 100px;
  text-align: center;
  -webkit-flex: 1;
  flex: 1;
  
  span
    color: #000;

  .front-title
    font-size: 2rem;

  .front-body
    margin-top: 40px;
    font-size: 1.5rem;

    .front-techs
      span:not(:last-child):after
        content: ',';
        padding-right: 4px;

    > span
      display: block;

    .front-operation
      font-size: 1.2rem;

  .front-button
    margin-top: 30px;
    
